<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
	<DataCollectionRunSettings>
		<DataCollectors>
		  <DataCollector friendlyName="Code Coverage" uri="datacollector://Microsoft/CodeCoverage/2.0" assemblyQualifiedName="Microsoft.VisualStudio.Coverage.DynamicCoverageDataCollector, Microsoft.VisualStudio.TraceCollector, Version=12.0.0.0, Culture=neutral">
			<Configuration>
			  <CodeCoverage>
				<ModulePaths>
				  <Include>
					<ModulePath>.*\.dll$</ModulePath>
					<ModulePath>.*\.exe$</ModulePath>
				  </Include>
				  <Exclude>
					<ModulePath>*newtonsoft*.dll</ModulePath>
				  </Exclude>
				</ModulePaths>
			  </CodeCoverage>
			</Configuration>
		  </DataCollector>
		</DataCollectors>
	  </DataCollectionRunSettings>
</RunSettings>